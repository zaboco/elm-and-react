{
  "name": "elm-and-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-elm-components": "^1.0.1",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-elm-middleware": "^3.0.0"
  },
  "devDependencies": {
    "flow-bin": "^0.43.1",
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn build-elm && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "flow": "flow",
    "build-elm": "cd elm && elm make Posts.elm --output=posts.elm.js",
    "build-elm-redux": "cd elm && elm make Reducer.elm --output=reducer.elm.js",
    "watch-elm": "chokidar 'elm/**/*.elm' -c 'yarn build-elm' --initial -d 1700",
    "watch-elm-redux": "chokidar 'elm/**/*.elm' -c 'yarn build-elm-redux' --initial -d 1700",
    "eject": "react-scripts eject"
  }
}
